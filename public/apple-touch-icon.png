<svg viewBox="0 0 180 180" xmlns="http://www.w3.org/2000/svg" width="180" height="180">
  <!-- Background with rounded corners -->
  <rect width="180" height="180" rx="40" fill="#3B82F6"/>

  <!-- Car icon scaled up -->
  <g transform="translate(54, 63)">
    <!-- Car body -->
    <rect x="4" y="20" width="64" height="18" rx="4" fill="white"/>
    <!-- Car roof -->
    <path d="M16 20 L24 4 L48 4 L56 20" fill="white"/>
    <!-- Wheels -->
    <circle cx="20" cy="42" r="6" fill="white" stroke="#3B82F6" stroke-width="2"/>
    <circle cx="52" cy="42" r="6" fill="white" stroke="#3B82F6" stroke-width="2"/>
    <!-- Windshield -->
    <rect x="26" y="6" width="20" height="12" rx="2" fill="#3B82F6"/>
  </g>

  <!-- Speedometer arc -->
  <g transform="translate(90, 90)">
    <path d="M -32 6 A 32 32 0 0 1 32 6" stroke="white" stroke-width="6" fill="none"/>
    <!-- Needle -->
    <line x1="0" y1="0" x2="22" y2="-12" stroke="white" stroke-width="4" stroke-linecap="round"/>
    <!-- Center dot -->
    <circle cx="0" cy="0" r="4" fill="white"/>
  </g>
</svg>